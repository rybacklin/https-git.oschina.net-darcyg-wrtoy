/dts-v1/;

/include/ "mt7620a.dtsi"

/ {
	compatible = "mtall", "ralink,mt7620a-soc";
	model = "MTALL";

	/*chosen {
		bootargs = "console=ttyS0,115200n8";
	};*/

	palmbus@10000000 {
		/* Manfeel: enabled gpio0 & gpio1 */
		gpio0: gpio@600 {
			status = "okay";
		};

		gpio1: gpio@638 {
			status = "okay";
		};

		gpio2: gpio@660 {
			status = "okay";
		};

		gpio3: gpio@688 {
			status = "okay";
		};

		i2c@900 {
			status = "okay";
		};

		spi@b00 {
			status = "okay";

			m25p80@0 {
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "w25q256";
				reg = <0 0>;
				linux,modalias = "m25p80", "w25q256";
				spi-max-frequency = <10000000>;

				partition@0 {
					label = "u-boot";
					reg = <0x0 0x30000>;
					read-only;
				};

				partition@30000 {
					label = "u-boot-env";
					reg = <0x30000 0x10000>;
					read-only;
				};

				factory: partition@40000 {
					label = "factory";
					reg = <0x40000 0x10000>;
					read-only;
				};

				partition@50000 {
					label = "firmware";
					reg = <0x50000 0x1fb0000>;
				};
			};

			/*
			m25p80@1 {
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "w25q256";
				reg = <1 0>;
				linux,modalias = "m25p80", "w25q256";
				spi-max-frequency = <10000000>;

				partition@0 {
					label = "edisk1";
					reg = <0x0 0x2000000>;
				};
			};

			m25p80@2 {
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "w25q256";
				reg = <2 0>;
				linux,modalias = "m25p80", "w25q256";
				spi-max-frequency = <10000000>;

				partition@0 {
					label = "edisk2";
					reg = <0x0 0x2000000>;
				};
			};
			*/

			/* Manfeel: enable spidev */
			/*
			spidev@1 {
				#address-cells = <1>;
				#size-cells = <1>;
				compatible = "spidev";
				reg = <1 0>;
				linux,modalias = "spidev", "spidev";
				spi-max-frequency = <10000000>;

			};
			*/
		};
	};

	ehci@101c0000 {
		status = "okay";
	};

	ohci@101c1000 {
		status = "okay";
	};

	sdhci@10130000 {
		status = "okay";
	};

	ethernet@10100000 {
		mtd-mac-address = <&factory 0x4>;
		ralink,port-map = "wllll";
	};

	wmac@10180000 {
		ralink,mtd-eeprom = <&factory 0>;
	};

	pinctrl {
		state_default: pinctrl0 {
			default {
				ralink,group = "wled", "pa", "wdt", "mdio";
				ralink,function = "gpio";
			};

			uartf_gpio {
			  ralink,group = "uartf";
			  ralink,function = "gpio uartf";
			};
		};
	};

	gpio-leds {
		compatible = "gpio-leds";
		wlan {
			label = "mtall:blue:wlan";
			gpios = <&gpio3 0 1>;
		};
	};

};
